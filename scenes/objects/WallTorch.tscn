script/source = "extends OmniLight

var start_range
var dest_range

var getting_to_range

func _ready():
	start_range = omni_range
	getting_to_range = false
	pass

func _process(delta):
	if !getting_to_range:
		dest_range = start_range + rand_range(-2,2)
		getting_to_range = true
		
	omni_range = omni_range + (dest_range - omni_range)*0.25
	if abs(omni_range - dest_range) < 0.01:
		getting_to_range = false
 	pass"

